-------------------------------------------------------------------------------
-- Syntax
-------------------------------------------------------------------------------

data HMTm
  | HMTm  pos :: {SourcePos}  t :: HMTm_

data HMTm_
  | Var  x :: {Var}
  | Lam  x :: {Var}  t1 :: HMTm
  | App  t1 :: HMTm  t2 :: HMTm
  | Let  x :: {Var}  t1 :: HMTm  t2 :: HMTm

